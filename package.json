{
  "name": "loopback-ds-ips-mixin",
  "version": "1.0.2",
  "description": "A mixin to automatically generate createdByIp and updatedByIp attributes for loopback Models",
  "main": "index.js",
  "scripts": {
    "preversion": "npm test",
    "pretest": "jscs ./es6/*.js && jshint ./es6/*.js && gulp babel",
    "test": "nyc tap ./test/*.js",
    "watch": "gulp",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "outdated": "npm outdated --depth=0"
  },
  "keywords": [
    "loopback",
    "strongloop",
    "mixin",
    "ip",
    "ip address"
  ],
  "author": "FluxAugur",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/FluxAugur/loopback-ds-ips-mixin.git"
  },
  "dependencies": {
    "babel-plugin-rewire": "^1.0.0-beta-3",
    "debug": "2.x",
    "ipware": "0.0.5"
  },
  "peerDependencies": {
    "loopback-datasource-juggler": ">=2.33.2"
  },
  "devDependencies": {
    "babel-core": "latest",
    "babel-plugin-object-assign": "latest",
    "coveralls": "latest",
    "gulp": "latest",
    "gulp-babel": "latest",
    "gulp-sourcemaps": "latest",
    "jscs": "latest",
    "jshint": "latest",
    "loopback": "2.x",
    "loopback-boot": "2.x",
    "nyc": "latest",
    "tap": "latest"
  }
}
